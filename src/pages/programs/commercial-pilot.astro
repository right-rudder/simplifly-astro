---
import BaseLayout from "../../layouts/BaseLayout.astro";
import HeaderSmall from "../../components/HeaderSmall.astro";
import CTAColor from "../../components/CTAColor.astro";
import flyWithUsCTA from "../../data/fly-with-us-cta";

import ProgramDetails from "../../components/ProgramDetails.astro";
import programData from "../../data/commercialPilotData.js";

---
<BaseLayout
  siteTitle={programData.pageTitle}
  siteDescription={programData.pageDescription}
  keywords={programData.pageKeywords}
  >
  
  <HeaderSmall content={programData.header} />

  <ProgramDetails program={programData.details} />

  <CTAColor content={flyWithUsCTA} />

</BaseLayout>

